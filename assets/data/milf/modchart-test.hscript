function beatHit(beat)
{
    if (beat >= 168 && beat < 200 && camZooming && FlxG.camera.zoom < 1.35) {
        FlxG.camera.zoom += 0.015;
        camHUD.zoom += 0.03;
    }

    switch(beat) {
	case 40:
	    switchCharacter('gf', 'dad');
	case 56:
	    switchCharacter('dad', 'bf');
	case 18*4:
	    switchCharacter('monster', 'dad');
	case 88:
	    switchCharacter('monster-christmas', 'bf');
	case 26*4:
	    switchCharacter('pico', 'dad');
	case 120:
	    switchCharacter('spooky', 'bf');
	case 34*4:
	    switchCharacter('senpai-angry', 'dad');
	case 38*4:
	    switchCharacter('senpai', 'bf');
	case 168:
	    switchCharacter('spirit', 'dad');
	case 46*4:
	    switchCharacter('matt-ghost', 'bf');
	case 200:
	    switchCharacter('mf-pixel', 'dad');
	    switchCharacter('gf-pixel', 'gf');
	case 54*4:
	    switchCharacter('bf-pixel', 'bf');
	case 58*4:
	    switchCharacter('bf-christmas', 'dad');
	    switchCharacter('gf-christmas', 'bf');
	case 66*4:
	    switchCharacter('mf', 'dad');
	case 280:
	    switchCharacter('bulb-pixel', 'bf');
	case 82*4:
	    switchCharacter('mom', 'dad');
	    switchCharacter('gf', 'gf');
	case 86*4:
	    switchCharacter('bf', 'bf');
    }
}

